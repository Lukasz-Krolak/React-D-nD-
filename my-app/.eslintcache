ne extends Component{\r\n    render(){\r\n        const { brand } = this.props;\r\n        const { isDragging, connectDragSource } = this.props;\r\n\r\n        const opacity = isDragging? 0.4: 1;\r\n        const style={\r\n            opacity: opacity\r\n        };\r\n        const phoneClass = isDragging? 'ui card phone drag': 'ui card phone';\r\n        return connectDragSource(\r\n            <div className={phoneClass} style={style}>\r\n                <div className=\"image\"><img src=\"/images/phone.jpg\" /></div>\r\n                <div className=\"content\">\r\n                    <div className=\"phone-name\">{ brand }</div>\r\n                    <div className=\"meta\">8G RAM, 16G memory</div>\r\n                </div>\r\n                <div className=\"extra content\">\r\n                    <a>\r\n                    <i aria-hidden=\"true\" className=\"money icon\"></i>\r\n                    $ 80\r\n                    </a>\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default connect()(DragSource(ItemTypes.PHONE, phoneSpec, collect)(Phone));",["74","75"],{"ruleId":"76","severity":1,"message":"77","line":48,"column":86,"nodeType":"78","messageId":"79","endLine":48,"endColumn":88},{"ruleId":"80","severity":1,"message":"81","line":1,"column":8,"nodeType":"82","messageId":"83","endLine":1,"endColumn":13},{"ruleId":"84","severity":1,"message":"85","line":50,"column":40,"nodeType":"86","endLine":50,"endColumn":71},{"ruleId":"87","severity":1,"message":"88","line":56,"column":21,"nodeType":"86","endLine":56,"endColumn":24},{"ruleId":"84","severity":1,"message":"85","line":47,"column":40,"nodeType":"86","endLine":47,"endColumn":71},{"ruleId":"87","severity":1,"message":"88","line":53,"column":21,"nodeType":"86","endLine":53,"endColumn":24},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","no-unused-vars","'React' is d